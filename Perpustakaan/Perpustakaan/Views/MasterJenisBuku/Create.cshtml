@model Perpustakaan.Models.MasterJenisBuku

<h2>Tambah Jenis Buku</h2>

<form method="post" asp-action="Create">
    <div class="form-group">
        <label for="JenisBuku">Jenis Buku:</label>
        <input asp-for="JenisBuku" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Simpan</button>
    <a href="/MasterJenisBuku" class="btn btn-secondary">Kembali</a>
</form>
